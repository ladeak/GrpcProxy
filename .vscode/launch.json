    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch ConsoleApp1",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/samples/ConsoleApp1/bin/Debug/net6.0/ConsoleApp1.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Launch WebApplication1",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/samples/WebApplication1/bin/Debug/net6.0/WebApplication1.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Launch Proxy",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/GrpcProxy/bin/Debug/net6.0/GrpcProxy.dll",
            "args": ["--contentRoot=/workspaces/GrpcProxy/src/GrpcProxy/bin/Debug/net6.0/"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        }

    ]
}